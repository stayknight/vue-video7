{
  "name": "vue-video7",
  "description": "A video.js 7 player component for Vue.js",
  "version": "1.0.1",
  "author": "stayknight <stayknight@163.com>",
  "homepage": "https://github.com/stayknight/vue-video7",
  "repository": {
    "type": "git",
